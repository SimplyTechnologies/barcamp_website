section.special-guests
    .container.container-fixed
        h1 Special guests
        .row
            ul
                .col-md-4.col-xs-4(ng-repeat="speaker in speakers")
                    li
                        img(ng-src="{{speaker.photo}}")
                        h3 {{speaker[currentLang].name}}
                        p
                            strong {{speaker[currentLang].topic}}
                        .user-hover
                            .social-icons
                                a(ng-if="speaker.socialnetworks.twitter", ng-href="{{speaker.socialnetworks.twitter}}", target='_blank' )
                                    i.fa.fa-twitter
                                a(ng-if="speaker.socialnetworks.facbook" ng-href="{{speaker.socialnetworks.facbook}}", target='_blank')
                                    i.fa.fa-facebook-official
                                a(ng-if="speaker.socialnetworks.linkedin" ng-href="{{speaker.socialnetworks.linkedin}}", target='_blank')
                                    i.fa.fa-linkedin-square
                                a(ng-if="speaker.socialnetworks.instagram" ng-href="{{speaker.socialnetworks.instagram}}", target='_blank')
                                    i.fa.fa-instagram


