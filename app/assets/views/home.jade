section.main-menu.build(ng-hide='daysLeft <= 0')
    .container.container-fixed
        .row
            .col-md-6.col-xs-6
                h2(translate='home.mostWantedEvent')
                h1(ng-cloak, ng-hide='daysLeft <= 0', translate='home.dayToGo' translate-values="{value: daysLeft}")
            .col-md-offset-6.col-xs-offset-6
.clearfix
section.schedule(ng-show='daysLeft <= 0')
    .container.container-fixed
        .row
            .col-md-12.col-xs-12
                h1 Schedule
                h2 Weâ€™ve created the product that
                    br
                    | will help your startup
    .container.schedule-container
        table.table
            thead
                tr
                    th(ng-repeat='room in rooms') {{room}}
            tbody
                tr(ng-repeat='shcedulesRow in shcedulesRows', ng-init='rowIndex = $index')
                    td(ng-repeat='shcedules in shcedulesRow', ng-class='{"bg-crimson-red": rowIndex === 0, "bg-blaze-orange": rowIndex === 1, "bg-feldgrau": rowIndex === 2}')
                        .schedule-img
                            img(ng-src="{{shcedules.bg_image_url}}")
                        .title
                            h2(translate='home.now', ng-show='rowIndex === 0')
                            h2(translate='home.next', ng-hide='rowIndex === 0')
                            .time
                                span {{shcedules.time_from.date | amDateFormat: 'HH:mm'}}
                                span  -
                                span {{shcedules.time_to.date | amDateFormat: 'HH:mm'}}
                            p.topic {{shcedules[currentLang].topic}}
                            .creator
                                span(translate='home.by')
                                span {{shcedules[currentLang].speaker}}
                    
        .to-full-schedule
            a(href='#') to full schedule >
.clearfix
.container-fluid.register
    .container.container-fixed
        h1.pull-left(translate='home.regForPartic', ng-class='{"reg-title-hy": currentLang == "hy"}')
        a.btn.btn-default.btn-orange.pull-right(href='https://www.eventbrite.co.uk/e/barcamp-yerevan-2015-tickets-16660816952', target='_blank', translate='home.register')
.clearfix
section.special-guests
    .container.container-fixed
        h1(translate='home.specialGuests')
        .row
            ul
                .col-md-4.col-xs-4(ng-repeat="speaker in speakers")
                    li
                        img(ng-src="{{speaker.photo.split('.').indexOf('png') !== -1 || speaker.photo.split('.').indexOf('jpg') !== -1 ? speaker.photo : '/build/assets/images/user-avatar.jpg'}}")
                        h3 {{speaker[currentLang].name}}
                        p
                            strong {{speaker[currentLang].topic}}
                        .user-hover
                            .social-icons
                                a(ng-if="speaker.socialnetworks.twitter", ng-href="{{speaker.socialnetworks.twitter}}", target='_blank' )
                                    i.fa.fa-twitter
                                a(ng-if="speaker.socialnetworks.facbook" ng-href="{{speaker.socialnetworks.facbook}}", target='_blank')
                                    i.fa.fa-facebook-official
                                a(ng-if="speaker.socialnetworks.linkedin" ng-href="{{speaker.socialnetworks.linkedin}}", target='_blank')
                                    i.fa.fa-linkedin-square
                                a(ng-if="speaker.socialnetworks.instagram" ng-href="{{speaker.socialnetworks.instagram}}", target='_blank')
                                    i.fa.fa-instagram

